package interfaces

interface Data {
	out event start
	out event continue
	out event anotherSignal
	out event pending
	out event data(value : boolean)
	out event integerData(value : integer)
}

interface TesterControl {
	out event next
	out event end
}

interface SchedulerControl {
	out event ready
	out event ignore
	out event end
}

interface ActivityControllerInterface {
	in persistent event control(isActive : boolean)
	out event done
}

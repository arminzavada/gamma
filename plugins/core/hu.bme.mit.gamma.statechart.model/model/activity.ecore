  <eClassifiers xsi:type="ecore:EClass" name="SynchronousActivityDefinition" eSuperTypes="composite.ecore#//SynchronousComponent #//ActivityDefinition"/>
  <eClassifiers xsi:type="ecore:EClass" name="TriggerNode" eSuperTypes="../../hu.bme.mit.gamma.activity.model/model/activity.ecore#//ControlNode">
